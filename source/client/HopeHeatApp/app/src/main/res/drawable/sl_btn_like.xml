<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/common_second_red" />
            <stroke android:width="1px" android:color="@color/common_main_red" />

        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/color_white" />
            <stroke android:width="1px" android:color="@color/vocal_indentify_color" />
        </shape>
    </item>
</selector>